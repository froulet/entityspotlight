{% extends 'base.html.twig' %}


{% block body %}

      <!-- TimeSheet -->
   <link href="{{ asset('bundles/css/timesheet.min.css') }}" type="text/css" rel="stylesheet" media="screen,projection"/>

   <script src="{{ asset('bundles/js/timesheet.min.js') }}"></script>


  <div class="section no-pad-bot  blue-grey lighten-5" id="index-banner">

    <div class="container">
      <div class="row center">


      <div class="col s12" style="opacity: 1;">
      <h2>{{category |replace({'_': " "}) }}</h2>
        <div id="timesheet"></div>
    </div>


</div>


</div>

</div>

    <script type="text/javascript">
    new Timesheet('timesheet', 2013, 2016, [

    {% for revision in revisions %}
      {% if revision.category_title == category %}
      ['{{revision.date|date('m/Y')}}', "{{revision.category_title}}"],
      {% else %}
      ['{{revision.date|date('m/Y')}}', "{{revision.category_title}}", "ipsum"],
      {% endif %}
    {% endfor %}
    ['{{revisions|first.date|date('m/Y')}}', '{{revisions|last.date|date('m/Y')}}', "{{entity.title}}", "ipsum"],


    ]);
    </script>

{% endblock %}
