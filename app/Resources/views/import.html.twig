{% extends 'base.html.twig' %}

{% block body %}

  <div class="section no-pad-bot  blue-grey lighten-5" id="index-banner">

    <div class="container">
      <br><br>
      <h1 class="header center orange-text">Add New Entities</h1>
      <div class="row center">

      {% for key, leimport in imported %}
            {% if leimport == '1' %}
            <p class="card-panel teal lighten-2" > {{key|replace({'_': " "})}} imported. </p>
            {% else %}
            <p class="card-panel red accent-4"> {{key|replace({'_': " "})}}  not imported. </p>
            {% endif %}
      {% endfor %}
        <h6> One entity's name per line </h6>
        {{ form(form) }}
      </div>


    </div>

  </div>

{% endblock %}
